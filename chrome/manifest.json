{
  "name": "Replace Google CDN",
  "version": "0.1",
  "manifest_version": 2,
  "description": "将GoogleCDN替换为国内CDN",
  "icons": {
    "16":  "data/icon-16.png",
    "48":  "data/icon-48.png",
    "128": "data/icon-128.png"
  },
  "background": {
    "scripts": ["js/background.js"]
  },
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "<all_urls>"
  ]
}
